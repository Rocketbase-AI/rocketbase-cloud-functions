{
  "name": "rocketbase-cloud-functions",
  "version": "2.0.0",
  "description": "RocketBase cloud functions",
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "scripts": {
    "logs": "npx firebase functions:log",
    "serve": "npm run build && npx firebase serve --only functions",
    "shell": "npm run build && npx firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "npx firebase deploy --only functions",
    "lint": "npx tslint -p tsconfig.json",
    "build": "npx tsc"
  },
  "private": true,
  "keywords": [
    "firebase",
    "cloud",
    "functions",
    "typescript"
  ],
  "author": "Mirage Technologies AG",
  "license": "UNLICENSED",
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "7.3.0",
    "firebase-functions": "^2.3.1",
    "mixpanel": "^0.10.2"
  },
  "devDependencies": {
    "firebase-tools": "^6.8.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}
