{
  "name": "rockethub",
  "version": "1.0.0",
  "description": "RocketHub web application",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "serve:funcs": "npm run build-functions && firebase serve --only functions",
    "dep": "firebase deploy",
    "dep:funcs": "firebase deploy --only functions",
    "clean": "rimraf \"build/functions\" && rimraf \"build/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/static/**/*.*\" \"src/public/static\" -C && cpx \"src/public/**/*.*\" \"build/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tslint --project src/functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"build/functions\" -C",
    "install-deps": "cd \"build/functions\" && npm i",
    "lint": "npm run lint-app && npm run lint-functions && npm run typecheck-app",
    "build": "npm run build-public && npm run build-functions && npm run build-app"
  },
  "private": true,
  "keywords": [
    "firebase",
    "next",
    "typescript"
  ],
  "author": "Heiki",
  "license": "UNLICENSED",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-regular-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bulma": "^0.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "firebase": "^6.0.1",
    "firebase-admin": "7.3.0",
    "firebase-functions": "^2.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "mixpanel": "^0.10.2",
    "moment": "^2.24.0",
    "next": "^8.1.0",
    "next-cookies": "^1.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.2",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "firebase-tools": "^6.9.1",
    "node-sass": "^4.12.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  }
}
